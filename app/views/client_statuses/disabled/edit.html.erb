<h1>Editing client_status</h1>

<% form_for(@client_status) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :status %><br />
    <%= f.text_field :status %>
  </p>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', @client_status %> |
<%= link_to 'Back', client_statuses_path %>
