<h1>Listing file_contents</h1>

<table>
  <tr>
    <th>Md5</th>
    <th>Sha1</th>
    <th>Size</th>
    <th>Mime type</th>
  </tr>

<% for file_content in @file_contents %>
  <tr>
    <td><%=h file_content.md5 %></td>
    <td><%=h file_content.sha1 %></td>
    <td><%=h file_content.size %></td>
    <td><%=h file_content.mime_type %></td>
    <td><%= link_to 'Show', file_content %></td>
    <td><%= link_to 'Edit', edit_file_content_path(file_content) %></td>
    <td><%= link_to 'Destroy', file_content, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New file_content', new_file_content_path %>
